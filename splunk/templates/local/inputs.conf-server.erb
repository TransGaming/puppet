[default]
host = <%= fqdn %>

[splunktcp-ssl:9997]
compressed = true

[SSL]
password = ssl password
requireClientCert = true
rootCA = $SPLUNK_HOME/etc/certs/cacert.pem
serverCert = $SPLUNK_HOME/etc/certs/splunk.pem
